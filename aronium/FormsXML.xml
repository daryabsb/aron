    private void _StartReport(object sender, EventArgs e)
    {
      if(((Boolean)Report.GetParameterValue(&quot;IsFooterCentered&quot;)))
        TextFooter.HorzAlign = HorzAlign.Center;
      
      RowCompanyBankAccountNumber.Visible = !string.IsNullOrEmpty(((String)Report.GetColumnValue(&quot;Company.BankAccountNumber&quot;))); 
      RowCompanyBankDetail.Visible = !string.IsNullOrEmpty(((String)Report.GetColumnValue(&quot;Company.BankDetails&quot;))); 
      RowCustomerTax.Visible = ((Boolean)Report.GetParameterValue(&quot;PrintCustomerTaxNumber&quot;));
      RowCustomerPhone.Visible = ((Boolean)Report.GetParameterValue(&quot;PrintCustomerPhoneNumber&quot;));
      RowCustomerEmail.Visible = ((Boolean)Report.GetParameterValue(&quot;PrintCustomerEmail&quot;));
      RowCustomerCode.Visible = ((Boolean)Report.GetParameterValue(&quot;PrintCustomerCode&quot;));
      DataPaymentTypes.Visible = DataHeaderPaymentTypes.Visible = DataFooterPaymentTypes.Visible = ((Boolean)Report.GetParameterValue(&quot;PrintPaymentTypes&quot;));
      
      CheckDiscountAndTaxColumns(); 
    }
    
    private void CheckDiscountAndTaxColumns()
    {
      bool printTaxColumn = (Boolean)Report.GetParameterValue(&quot;PrintTaxColumn&quot;);
      bool printDiscountColumn = ((Boolean)Report.GetParameterValue(&quot;PrintDiscountColumn&quot;));
      
      if(!printTaxColumn || !printDiscountColumn)
      {
        float taxShiftWidth = printTaxColumn ? 0f : TextTaxHeader.Width;
        float discountShiftWidth = printDiscountColumn ? 0f : TextDiscountHeader.Width;
      
        float width = taxShiftWidth + discountShiftWidth;
      
        TextTax.Visible = TextTaxHeader.Visible = printTaxColumn;
        TextDiscount.Visible = TextDiscountHeader.Visible = printDiscountColumn;
        
        TextPriceHeader.Left = TextPrice.Left -= width;
        TextQuantityHeader.Left = TextQuantity.Left -= width;
        TextProductNameHeader.Left = TextProductName.Left -= width;
        TextProductNameHeader.Width = TextProductName.Width += width;
        
        if(printTaxColumn &amp;&amp; !printDiscountColumn)
        {
          TextTaxHeader.Left = TextTax.Left -= width;  
        }
      }
    }                        
    
    private void OnDiscountBeforePrint(object sender, EventArgs e)
    {
      DataSourceBase ds = Report.GetDataSource(&quot;Taxes&quot;);
      
      ds.Init();
      
      int decimalPlaces = System.Threading.Thread.CurrentThread.CurrentCulture.NumberFormat.NumberDecimalDigits;
      
      double subtotal = Convert.ToDouble(Report.GetColumnValue(&quot;Document.Total&quot;));
      
      if(ds.RowCount &gt; 0)
      {
        while (ds.HasMoreRows)
        {
          subtotal -= Math.Round(Convert.ToDouble(ds[&quot;Amount&quot;]), decimalPlaces);
          
          ds.Next();
        }
        TextSubtotal.Text = subtotal.ToString(&quot;C&quot; + decimalPlaces);
      }
      else
      {
        TextSubtotalLabel.Visible = TextSubtotal.Visible = false;
      }
      
      PrintDiscount(subtotal, decimalPlaces);
      
      if(Engine.FinalPass)
      {
        if(!TextDiscountLabel.Visible)
        {
          TextSubtotalSeparator.Top = TextDiscountLabel.Top;
        
          TextSubtotalSeparator.Top = 0;
          TextSubtotalSeparator.Border.Lines = BorderLines.None; 
          if(TextSubtotalLabel.Visible)
          {
            TextSubtotalLabel.Top = TextSubtotal.Top = TextSubtotalSeparator.Bottom;
          }
        }
      }
    }
    
    private void PrintDiscount(double subtotal, int decimalPlaces)
    {
      double totalDiscount = (subtotal - (double)Report.GetTotalValue(&quot;ItemsSubtotal&quot;));
      // In case discount apply rule is before tax and percentage discount is used, print discount amount based on subtotal
      if(totalDiscount &gt; 0 &amp;&amp; ((int)Report.GetColumnValue(&quot;Document.DiscountApplyRule&quot;)) == 1 &amp;&amp; ((int)Report.GetColumnValue(&quot;Document.DiscountType&quot;)) == 0){
        TextDiscountAmount.Text = totalDiscount.ToString(&quot;C&quot; + decimalPlaces);
      }
      
      TextDiscountLabel.Visible = TextDiscountAmount.Visible = ((Double)Report.GetColumnValue(&quot;Document.Discount&quot;)) &gt; 0;
    }
    
    private string Address(string address, string zip, string city){
      return string.Format(&quot;{0}{1}{2} {3}&quot;, 
        address, 
        !string.IsNullOrEmpty(zip) || !string.IsNullOrEmpty(city) ? &quot;, &quot; : null, 
        zip,
        city).Trim();
    }
  }
}
