      <TableDataSource Name="Table" Alias="Sales" DataType="System.Int32" Enabled="true" SelectCommand="SELECT  U.Id as UserId, &#13;&#10;        U.FirstName, &#13;&#10;        U.LastName,  &#13;&#10;        PT.Id AS PaymentTypeId,&#13;&#10;        PT.Name AS PaymentTypeName, &#13;&#10;        SUM(CASE WHEN DT.StockDirection=1 THEN -P.Amount ELSE P.Amount END) AS Total&#13;&#10;        FROM    [User] U &#13;&#10;        INNER JOIN Document D ON D.UserId = U.Id&#13;&#10;        INNER JOIN DocumentType DT ON D.DocumentTypeId = DT.Id&#13;&#10;        INNER JOIN Payment P ON P.DocumentId = D.Id&#13;&#10;        INNER JOIN PaymentType PT ON PT.Id = P.PaymentTypeId&#13;&#10;WHERE   DT.DocumentCategoryId = 2 AND D.CashRegisterId=@CashRegisterId AND D.Id &gt;= (SELECT FromDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND D.Id &lt;= (SELECT ToDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number)&#13;&#10;        GROUP   BY U.Id, U.FirstName, U.LastName, PT.Id, PT.Name, DT.StockDirection &#13;&#10;ORDER   BY DT.StockDirection DESC   ">
        <Column Name="FirstName" DataType="System.String"/>
        <Column Name="LastName" DataType="System.String"/>
        <Column Name="Total" DataType="System.Object"/>
        <Column Name="UserId" DataType="System.Int64"/>
        <Column Name="PaymentTypeId" DataType="System.Int64"/>
        <Column Name="PaymentTypeName" DataType="System.String"/>
        <CommandParameter Name="Number" DataType="8" Expression="[Number]" DefaultValue="1"/>
        <CommandParameter Name="CashRegisterId" DataType="8" Expression="[CashRegisterId]" DefaultValue="1"/>
      </TableDataSource>
      <TableDataSource Name="Table1" Alias="ReportDocuments" DataType="System.Int32" Enabled="true" SelectCommand="SELECT  SUBSTRING(MIN(D.Number),8,LEN(MIN(D.Number))-7) AS FromDocument,&#13;&#10;        SUBSTRING(MAX(D.Number),8,LEN(MAX(D.Number))-7) AS ToDocument,&#13;&#10;        MIN(Date) AS BusinessDay&#13;&#10;FROM    Document D &#13;&#10;        INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId &#13;&#10;WHERE   DT.DocumentCategoryId = 2 &#13;&#10;        AND DT.StockDirection = 2 &#13;&#10;        AND D.CashRegisterId=@CashRegisterId AND D.Id &gt;= (SELECT FromDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND D.Id &lt;= (SELECT ToDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number)">
        <Column Name="FromDocument" DataType="System.String"/>
        <Column Name="ToDocument" DataType="System.String"/>
        <Column Name="BusinessDay" DataType="System.DateTime"/>
        <CommandParameter Name="Number" DataType="8" Expression="[Number]" DefaultValue="1"/>
        <CommandParameter Name="CashRegisterId" DataType="8" Expression="[CashRegisterId]" DefaultValue="1"/>
      </TableDataSource>
      <TableDataSource Name="Table3" Alias="CashIn" DataType="System.Int32" Enabled="true" SelectCommand="SELECT  SUM(CASE WHEN StartingCashType = 0 THEN Amount ELSE 0 END) AS CashIn,&#13;&#10;        SUM(CASE WHEN StartingCashType = 1 THEN -Amount ELSE 0 END) AS CashOut&#13;&#10;FROM    StartingCash WHERE ZReportNumber=@Number AND CashRegisterId=@CashRegisterId">
        <Column Name="CashIn" DataType="System.Object"/>
        <Column Name="CashOut" DataType="System.Object"/>
        <CommandParameter Name="Number" DataType="8" Expression="[Number]" DefaultValue="1"/>
        <CommandParameter Name="CashRegisterId" DataType="8" Expression="[CashRegisterId]" DefaultValue="1"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="ZReportNumber" DataType="System.Int32" Enabled="true" SelectCommand="SELECT Number, DateCreated FROM ZReport WHERE CashRegisterId=@CashRegisterId AND  Number=@Number">
        <Column Name="Number" DataType="System.Object"/>
        <Column Name="DateCreated" DataType="System.DateTime"/>
        <CommandParameter Name="Number" DataType="8" Expression="[Number]" DefaultValue="1"/>
        <CommandParameter Name="CashregisterId" DataType="8" Expression="[CashRegisterId]" DefaultValue="1"/>
      </TableDataSource>
      <TableDataSource Name="Table4" Alias="DocumensCount" DataType="System.Int32" Enabled="true" SelectCommand="SELECT  SUM(CASE WHEN DT.StockDirection = 2 THEN 1 ELSE 0 END) AS NumberOfSales,&#13;&#10;        SUM(CASE WHEN DT.StockDirection = 1 THEN 1 ELSE 0 END) AS NumberOfRefunds&#13;&#10;FROM    Document D &#13;&#10;        INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId &#13;&#10;WHERE   DT.DocumentCategoryId = 2 &#13;&#10;        AND D.CashRegisterId=@CashRegisterId AND D.Id &gt;= (SELECT FromDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND D.Id &lt;= (SELECT ToDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number)">
        <Column Name="NumberOfSales" DataType="System.Object"/>
        <Column Name="NumberOfRefunds" DataType="System.Object"/>
        <CommandParameter Name="Number" DataType="8" Expression="[Number]" DefaultValue="1"/>
        <CommandParameter Name="CashRegisterId" DataType="8" Expression="[CashRegisterId]" DefaultValue="1"/>
      </TableDataSource>
      <TableDataSource Name="Table5" Alias="Discounts" DataType="System.Int32" Enabled="true" SelectCommand="SELECT ISNULL(SUM(T.TotalDiscount),0.00) AS TotalDiscount FROM &#13;&#10;(&#13;&#10;    SELECT CASE WHEN DT.StockDirection=1 THEN ROUND(-1,2) ELSE ROUND(1,2) END * &#13;&#10; 	CASE WHEN D.DiscountType = 0 THEN &#13;&#10;	    CASE WHEN D.Discount = 100 THEN &#13;&#10;	    (&#13;&#10;	        SELECT SUM(DI.Price * DI.Quantity) FROM DocumentItem DI WHERE DI.DocumentId = D.Id&#13;&#10;	    ) &#13;&#10;	    ELSE (D.Total / ((ROUND(100,2) - D.Discount) / ROUND(100,2))) - D.Total END &#13;&#10;	ELSE D.Discount END AS TotalDiscount&#13;&#10;    FROM Document D INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId &#13;&#10;    WHERE DT.DocumentCategoryId = 2 AND D.CashRegisterId=@CashRegisterId AND D.Id &gt;= (SELECT FromDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND D.Id &lt;= (SELECT ToDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND D.Discount &gt; 0&#13;&#10;    UNION ALL&#13;&#10;    SELECT CASE WHEN DT.StockDirection=1 THEN ROUND(-1,2) ELSE ROUND(1,2) END *&#13;&#10;        CASE WHEN DI.DiscountType = 0 THEN &#13;&#10;            CASE WHEN DI.Discount = 100 THEN &#13;&#10;                (DI.Price * DI.Quantity)&#13;&#10;            ELSE &#13;&#10;                (DI.Total / ((ROUND(100,2) - DI.Discount) / ROUND(100,2))) - DI.Total END &#13;&#10;        ELSE DI.Discount END AS TotalDiscount&#13;&#10;    FROM DocumentItem DI INNER JOIN Document D ON D.Id = DI.DocumentId INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId &#13;&#10;    WHERE DT.DocumentCategoryId = 2 AND D.CashRegisterId=@CashRegisterId AND D.Id &gt;= (SELECT FromDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND D.Id &lt;= (SELECT ToDocumentId FROM ZReport WHERE CashRegisterId=@CashRegisterId AND Number=@Number) AND DI.Discount &gt; 0&#13;&#10;) AS T">
        <Column Name="TotalDiscount" DataType="null"/>
        <CommandParameter Name="Number" DataType="8" Expression="[Number]" DefaultValue="1"/>
        <CommandParameter Name="CashRegisterId" DataType="8" Expression="[CashRegisterId]" DefaultValue="1"/>
      </TableDataSource>
      <TableDataSource Name="Table6" Alias="POS" DataType="System.Int32" Enabled="true" SelectCommand="SELECT Name FROM CashRegister WHERE Id=1">
        <Column Name="Name" DataType="System.String"/>
