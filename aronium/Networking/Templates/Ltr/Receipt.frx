      <TableDataSource Name="Table2" Alias="Document" DataType="System.Int32" Enabled="true" SelectCommand="SELECT  D.Date, &#13;&#10;        D.DueDate,&#13;&#10;        D.DateCreated,&#13;&#10;        D.Number,&#13;&#10;        D.OrderNumber,&#13;&#10;        D.ReferenceDocumentNumber,&#13;&#10;        D.Note,&#13;&#10;        C.Id CustomerId,&#13;&#10;        C.Name CustomerName,&#13;&#10;        C.Code CustomerCode,&#13;&#10;        C.PhoneNumber CustomerPhoneNumber,&#13;&#10;        C.Email CustomerEmail,&#13;&#10;        C.Address CustomerAddress,&#13;&#10;        C.PostalCode CustomerPostalCode,&#13;&#10;        C.TaxNumber CustomerTaxNumber,&#13;&#10;        C.City CustomerCity,&#13;&#10;        D.Discount,&#13;&#10;        D.DiscountType,&#13;&#10;        D.DiscountApplyRule,&#13;&#10;        D.Total * (CASE WHEN DT.StockDirection = 1 THEN -1.00 ELSE 1.00 END) AS Total,&#13;&#10;        D.PaidStatus,&#13;&#10;        U.FirstName,&#13;&#10;        U.LastName,&#13;&#10;        CAST(CASE WHEN DT.StockDirection = 1 THEN 1 ELSE 0 END AS BIT) AS IsRefund&#13;&#10;FROM    Document D &#13;&#10;        INNER JOIN Customer C ON C.Id = D.CustomerId &#13;&#10;        INNER JOIN [User] U ON U.Id = D.UserId &#13;&#10;        INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId&#13;&#10;WHERE  D.Id=@Id">
