      <TableDataSource Name="Table" Alias="Items" DataType="System.Int32" Enabled="true" SelectCommand="SELECT    C.Id,&#13;&#10; SUBSTRING(C.Name, 0, 50) AS Name,&#13;&#10; PT.Name AS PaymentType,&#13;&#10; SUM(CASE WHEN DT.StockDirection=1 THEN -P.Amount ELSE P.Amount END) Total&#13;&#10;FROM    Payment P INNER JOIN&#13;&#10;        Document D ON D.Id = P.DocumentId INNER JOIN &#13;&#10;        PaymentType PT ON Pt.Id = P.PaymentTypeId INNER JOIN&#13;&#10;        DocumentType DT ON DT.Id = D.DocumentTypeId  INNER JOIN&#13;&#10;        Customer C ON C.Id = D.CustomerId INNER JOIN &#13;&#10;        [User] U ON U.Id = D.UserId&#13;&#10;WHERE   DT.DocumentCategoryId = 2 AND DT.StockDirection &gt; 0 &#13;&#10;        AND (@CustomerId IS NULL OR D.CustomerId = @CustomerId)&#13;&#10;        AND (@UserId IS NULL OR D.UserId = @UserId)&#13;&#10;        AND (@CashRegisterId IS NULL OR D.CashRegisterId = @CashRegisterId)&#13;&#10;        AND D.Date BETWEEN @Start AND @End&#13;&#10;GROUP BY C.id, C.Name, P.PaymentTypeId, PT.Name">
