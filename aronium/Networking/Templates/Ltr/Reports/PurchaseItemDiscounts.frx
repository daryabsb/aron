      <TableDataSource Name="Table" Alias="Items" DataType="System.Int32" Enabled="true" SelectCommand=";WITH cte(Id)&#13;&#10;AS&#13;&#10;(&#13;&#10;    SELECT @ProductGroupId&#13;&#10;    UNION ALL&#13;&#10;    SELECT ProductGroup.Id &#13;&#10;    FROM ProductGroup INNER JOIN cte ON cte.Id = ProductGroup.ParentGroupId&#13;&#10;    WHERE @IncludeSubgroups = 1&#13;&#10;)&#13;&#10;&#13;&#10;SELECT P.Id,&#13;&#10;       P.Code, &#13;&#10;       P.Name,&#13;&#10;       SUM(CASE WHEN DT.StockDirection=2 THEN ROUND(-1,2) ELSE ROUND(1,2) END * CASE WHEN DI.DiscountType = 0 THEN (DI.Total / ((ROUND(100,2) - DI.Discount) / ROUND(100,2))) - DI.Total ELSE DI.Discount END) AS TotalDiscount &#13;&#10;FROM DocumentItem DI&#13;&#10; INNER JOIN Product P ON P.Id = DI.ProductId &#13;&#10;     INNER JOIN Document D ON D.Id = DI.DocumentId &#13;&#10;     INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId &#13;&#10;     INNER JOIN Customer C ON C.Id = D.CustomerId&#13;&#10;     INNER JOIN [User] U ON U.Id = D.UserId    &#13;&#10;WHERE   DT.DocumentCategoryId = 1  AND DI.Discount &gt; 0&#13;&#10;        AND (@ProductId IS NULL OR P.Id = @ProductId)&#13;&#10;        AND (@CustomerId IS NULL OR D.CustomerId = @CustomerId)&#13;&#10;        AND (@UserId IS NULL OR D.UserId = @UserId)&#13;&#10;        AND ((@ProductGroupId IS NULL AND (@IncludeSubgroups = 1 OR P.ProductGroupId IS NULL)) OR P.ProductGroupId IN (SELECT * FROM cte))&#13;&#10;        AND D.Date BETWEEN @Start AND @End &#13;&#10;GROUP BY P.Id, P.code, P.Name">
