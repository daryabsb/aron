      <TableDataSource Name="Table" Alias="Items" DataType="System.Int32" Enabled="true" SelectCommand="SELECT D.Id,&#13;&#10;    D.Number,&#13;&#10;    D.Date,&#13;&#10;    (D.Total - SUM(COALESCE(T.TotalTax, 0))) * CASE WHEN DT.StockDirection = 2 THEN CAST(1.00 AS MONEY) ELSE CAST(-1.00 AS MONEY) END AS Total,&#13;&#10;    C.Id AS CustomerId,&#13;&#10;    C.Name AS Customer,&#13;&#10;    RTRIM(COALESCE(U.FirstName, '') + ' ' + COALESCE(U.LastName,'')) AS [User],&#13;&#10;    SUM((DI.PriceBeforeTax * DI.Quantity) - (DI.TotalAfterDocumentDiscount - COALESCE(T.TotalTax, 0))) * CASE WHEN DT.StockDirection = 2 THEN CAST(1.00 AS MONEY) ELSE CAST(-1.00 AS MONEY) END AS TotalDiscount&#13;&#10;FROM Document D&#13;&#10;    INNER JOIN DocumentType DT ON DT.Id = D.DocumentTypeId&#13;&#10;    INNER JOIN Customer C ON C.Id = D.CustomerId&#13;&#10;    INNER JOIN [User] U ON U.Id = D.UserId&#13;&#10;    INNER JOIN DocumentItem DI ON DI.DocumentId = D.Id&#13;&#10;    LEFT JOIN&#13;&#10;    (&#13;&#10;        SELECT DI.Id, SUM(DIT.Amount) TotalTax&#13;&#10;        FROM DocumentItemTax DIT&#13;&#10;        INNER JOIN DocumentItem DI ON DI.Id = DIT.DocumentItemId&#13;&#10;        GROUP BY DI.Id&#13;&#10;    ) AS T ON T.Id = DI.Id&#13;&#10;WHERE DT.DocumentCategoryId=2 AND DT.StockDirection &gt; 0 &#13;&#10;    AND (@CustomerId IS NULL OR D.CustomerId = @CustomerId)&#13;&#10;    AND (@UserId IS NULL OR D.UserId = @UserId)&#13;&#10;    AND (@CashRegisterId IS NULL OR D.CashRegisterId = @CashRegisterId)&#13;&#10;    AND D.Date BETWEEN @Start AND @End&#13;&#10; AND (D.Discount &gt; 0 OR DI.Discount &gt; 0)&#13;&#10;GROUP BY D.Id, D.Number, D.Date, D.Total, C.Id, C.Name, U.FirstName, U.LastName, DT.StockDirection">
